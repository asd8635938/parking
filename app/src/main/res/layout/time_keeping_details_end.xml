<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <include layout="@layout/title_bar" android:id="@+id/title"/>

    <TextView
        android:id="@+id/indent_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#222222"
        android:textSize="@dimen/sp15"
        android:text="订单结束"
        android:layout_marginTop="@dimen/dp30"
        android:layout_marginLeft="@dimen/dp13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/title"
        />


    <TextView
        android:id="@+id/present_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#222222"
        android:textSize="@dimen/sp15"
        android:text="当前状态"
        android:layout_marginTop="@dimen/dp30"
        android:layout_marginLeft="@dimen/dp13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/indent_end"
        />
    <TextView
        android:id="@+id/present_state_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#27c38a"
        android:textSize="@dimen/sp15"
        android:text="使用中"
        android:gravity="right"
        android:layout_marginRight="@dimen/dp13"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/present_state"
        />

    <TextView
        android:id="@+id/timing_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#222222"
        android:textSize="@dimen/sp15"
        android:text="订单号"
        android:layout_marginTop="@dimen/dp10"
        android:layout_marginLeft="@dimen/dp13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/present_state"
        />
    <TextView
        android:id="@+id/timing_number_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#898989"
        android:textSize="@dimen/sp15"
        android:gravity="right"
        android:text="64489585"
        android:layout_marginRight="@dimen/dp13"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/timing_number"
        />
    <TextView
        android:id="@+id/parking_lock_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#222222"
        android:textSize="@dimen/sp15"
        android:text="车锁ID"
        android:layout_marginTop="@dimen/dp10"
        android:layout_marginLeft="@dimen/dp13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/timing_number"
        />
    <TextView
        android:id="@+id/parking_lock_id_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#898989"
        android:textSize="@dimen/sp15"
        android:gravity="right"
        android:text="64489468546585"
        android:layout_marginRight="@dimen/dp13"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/parking_lock_id"
        />

    <TextView
        android:id="@+id/use_station"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#222222"
        android:textSize="@dimen/sp15"
        android:text="使用地点"
        android:layout_marginTop="@dimen/dp10"
        android:layout_marginLeft="@dimen/dp13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/parking_lock_id"
        />

    <TextView
        android:id="@+id/use_station_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#898989"
        android:textSize="@dimen/sp15"
        android:gravity="right"
        android:text="南昌吾悦广场"
        android:layout_marginRight="@dimen/dp13"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/use_station"
        />
    <TextView
        android:id="@+id/parking_space_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#222222"
        android:textSize="@dimen/sp15"
        android:text="车位类型"
        android:layout_marginTop="@dimen/dp10"
        android:layout_marginLeft="@dimen/dp13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/use_station"
        />

    <TextView
        android:id="@+id/parking_space_type_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#898989"
        android:textSize="@dimen/sp15"
        android:gravity="right"
        android:text="充电桩车位"
        android:layout_marginRight="@dimen/dp13"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/parking_space_type"
        />
    <TextView
        android:id="@+id/parking_space_serial_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#222222"
        android:textSize="@dimen/sp15"
        android:text="车位编号"
        android:layout_marginTop="@dimen/dp10"
        android:layout_marginLeft="@dimen/dp13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/parking_space_type"
        />
    <TextView
        android:id="@+id/parking_space_serial_number_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#898989"
        android:textSize="@dimen/sp15"
        android:gravity="right"
        android:text="MX003"
        android:layout_marginRight="@dimen/dp13"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/parking_space_serial_number"
        />

    <TextView
        android:id="@+id/pay_cost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#222222"
        android:textSize="@dimen/sp15"
        android:text="费用"
        android:layout_marginTop="@dimen/dp10"
        android:layout_marginLeft="@dimen/dp13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/parking_space_serial_number"
        />

    <TextView
        android:id="@+id/pay_cost_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#898989"
        android:textSize="@dimen/sp15"
        android:gravity="right"
        android:text="30元"
        android:layout_marginRight="@dimen/dp13"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/pay_cost"
        />
    <Button
        android:id="@+id/pay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="@dimen/sp15"
        android:text="支付"
        android:background="@drawable/login_btn"
        android:layout_marginRight="@dimen/dp24"
        android:layout_marginLeft="@dimen/dp24"
        android:layout_marginTop="@dimen/dp70"
        app:layout_constraintTop_toBottomOf="@id/pay_cost"
        />












</android.support.constraint.ConstraintLayout>